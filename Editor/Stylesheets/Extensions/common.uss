.popup-background {
    background-color: rgba(0, 0, 0, 200);
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    align-items: center; /*Horizontal center*/
    justify-content: center; /*Vertical center*/
}

.popup-box {
    background-color: rgb(11, 21, 24);
    border-color: var(--color-blue);
    border-width: 2px;
    border-radius: var(--popup-border-radius);
    padding: 40px;
    align-items: center;
    max-width: 70%;
}

.popup-title {
    color: var(--color-blue);
}

.popup-text {
    font-size: 12px;
    white-space: normal;
    -unity-text-align: middle-center;
}

.popup-close-button {
    border-width: 0px;
    align-self: flex-end;
    position: absolute;
    width: 23px;
    height: 23px;
    border-radius: 5px;
    align-content: center;
    justify-content: center;
    right: 6px;    /*-4px to make it align with right -> 10 - 4 = 6px*/
    top: 8px;      /*-2px to make it align iwht top  -> 10 - 2 = 8px*/
}

.popup-deploy-warning-container {
    margin-top: 25px;
    border-radius: 10px;
    padding: 25px;
    background-color: rgb(40,40,40);
}